    "global": "~4.4.0",
    "intersection-observer": "~0.12.2",
    "prop-types": "~15.8.1",
    "react-intersection-observer": "~9.5.2"
  },
  "peerDependencies": {
    "react": "^18.x"
  },
  "devDependencies": {
    "husky": "^8.0.3",
    "nwb": "0.25.x",
    "prettier": "^3.0.0",
    "pretty-quick": "^3.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hanford/react-drag-drawer.git"
  },
  "homepage": "https://github.com/hanford/react-drag-drawer",
